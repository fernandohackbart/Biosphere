<?xml version="1.0" encoding="UTF-8" ?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:biosphereCellInterface="http://www.biosphere.org/tissue/1.0/cell/interface"
            targetNamespace="http://www.biosphere.org/tissue/1.0/cell/interface" elementFormDefault="qualified">
  <xsd:complexType name="cellinterfacetype">
    <xsd:sequence>
      <xsd:element name="cellport" minOccurs="1" maxOccurs="1" type="xsd:integer"/>
      <xsd:element name="celladdresses">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="celladdress" minOccurs="1" maxOccurs="unbounded" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
